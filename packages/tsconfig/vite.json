{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Vite",
    "extends": "./base.json",
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "allowJs": false,
        "allowSyntheticDefaultImports": true,
        "module": "ESNext",
        "noEmit": true,
        "jsx": "react-jsx"
    }
}